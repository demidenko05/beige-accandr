<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
  <!-- All android generated sources: -->
  <Match>
    <Class name="~.*R\$.*"/>
  </Match>
  <Match>
    <Class name="org.beigesoft.accandr.Bsa"/>
    <Bug pattern="ST_WRITE_TO_STATIC_FROM_INSTANCE_METHOD"/>
  </Match>
  <!--getActivity() never return null-->
  <Match>
    <Class name="org.beigesoft.accandr.Priv"/>
    <Bug pattern="NP_NULL_ON_SOME_PATH_FROM_RETURN_VALUE"/>
  </Match>
</FindBugsFilter>
